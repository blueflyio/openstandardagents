{
  "name": "@ossa/orchestra",
  "version": "0.1.8",
  "description": "OSSA Orchestra - Advanced Multi-Agent Workflow Orchestration Platform",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "test": "vitest",
    "lint": "eslint src/**/*.ts",
    "docker:build": "docker build -t ossa-orchestra:0.1.8 .",
    "docker:run": "docker run -p 3013:3013 ossa-orchestra:0.1.8"
  },
  "keywords": ["ossa", "orchestra", "workflow", "orchestration", "multi-agent", "scaling", "load-balancing"],
  "license": "Apache-2.0",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "winston": "^3.11.0",
    "node-cron": "^3.0.3",
    "ws": "^8.14.2",
    "uuid": "^9.0.1",
    "redis": "^4.6.10",
    "prometheus-api-metrics": "^3.2.2",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "typescript": "^5.3.2",
    "tsx": "^4.6.2",
    "@types/node": "^20.10.0",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/ws": "^8.5.9",
    "@types/uuid": "^9.0.7",
    "vitest": "^1.1.0",
    "eslint": "^8.56.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "config": {
    "port": 3013,
    "endpoint": "/api/v1/orchestra"
  }
}