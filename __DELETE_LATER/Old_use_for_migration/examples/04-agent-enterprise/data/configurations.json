{
  "agent_config": {
    "version": "1.0.0",
    "defaults": {
      "timeout_seconds": 300,
      "max_retries": 3,
      "rate_limit_per_minute": 100,
      "concurrent_requests": 10
    },
    "security": {
      "require_authentication": true,
      "allowed_origins": ["*"],
      "rate_limiting": true,
      "encryption": {
        "at_rest": true,
        "in_transit": "tls_1_3"
      }
    },
    "performance": {
      "cache_enabled": true,
      "cache_duration_seconds": 3600,
      "max_concurrent_requests": 10,
      "token_optimization": {
        "enabled": true,
        "target_savings": "35-45%",
        "strategies": [
          "request_deduplication",
          "response_compression",
          "context_window_optimization",
          "smart_caching"
        ]
      }
    },
    "monitoring": {
      "metrics_enabled": true,
      "logging_level": "info",
      "health_check_interval": 60,
      "alerting": {
        "enabled": true,
        "channels": ["email", "slack"],
        "thresholds": {
          "error_rate": 0.01,
          "response_time": 1000,
          "availability": 0.99
        }
      }
    }
  },
  
  "testing_config": {
    "test_execution": {
      "default_timeout": 300,
      "max_concurrent_tests": 5,
      "retry_failed_tests": true,
      "test_environments": ["development", "staging", "production"],
      "coverage_requirements": {
        "minimum": 80,
        "target": 90,
        "critical_paths": 95
      }
    },
    "validation": {
      "strict_mode": false,
      "include_suggestions": true,
      "frameworks": ["iso-42001", "nist-ai-rmf", "eu-ai-act"],
      "compliance_levels": ["bronze", "silver", "gold"]
    },
    "analysis": {
      "code_analysis": {
        "languages": ["typescript", "javascript", "python", "java", "csharp"],
        "analysis_types": ["quality", "security", "performance", "maintainability"],
        "include_suggestions": true
      },
      "performance_benchmarking": {
        "default_duration": 60,
        "concurrent_users": 10,
        "metrics": ["response_time", "throughput", "memory", "cpu", "error_rate"]
      }
    }
  },
  
  "framework_integrations": {
    "langchain": {
      "tool_type": "structured",
      "async_support": true,
      "streaming": true,
      "memory": "conversational",
      "chains": ["test_execution", "validation_analysis"]
    },
    "crewai": {
      "role": "test_specialist",
      "backstory": "Expert in testing, validation, and quality assurance",
      "delegation": true,
      "collaboration": "sequential"
    },
    "autogen": {
      "agent_type": "assistant",
      "system_message": "You are a comprehensive test agent specializing in OAAS validation and analysis",
      "conversation_style": "professional",
      "code_execution": true
    },
    "openai": {
      "assistant_type": "function_calling",
      "functions": ["execute_test", "validate_compliance", "analyze_code"],
      "file_search": true,
      "code_interpreter": true
    },
    "anthropic": {
      "mcp_server": true,
      "tools": ["test_execution", "validation_analysis"],
      "streaming": true,
      "resource_access": true
    },
    "google": {
      "vertex_ai": true,
      "function_calling": true,
      "gemini_integration": "native",
      "multimodal": true
    }
  },
  
  "protocol_bridges": {
    "mcp": {
      "enabled": true,
      "version": "2024-11-05",
      "capabilities": ["tools", "resources", "prompts"],
      "tools": [
        {
          "name": "execute_test",
          "description": "Execute a test scenario",
          "input_schema": {
            "type": "object",
            "properties": {
              "test_type": {"type": "string"},
              "parameters": {"type": "object"}
            }
          }
        },
        {
          "name": "validate_compliance",
          "description": "Validate OAAS compliance",
          "input_schema": {
            "type": "object",
            "properties": {
              "specification": {"type": "string"},
              "framework": {"type": "string"}
            }
          }
        }
      ],
      "resources": [
        {
          "name": "test_scenarios",
          "type": "file",
          "path": "./data/test-scenarios.json"
        },
        {
          "name": "validation_rules",
          "type": "file",
          "path": "./data/validation-rules.json"
        }
      ]
    },
    "a2a": {
      "enabled": true,
      "agent_card": {
        "name": "test-agent",
        "capabilities": ["testing", "validation", "analysis"],
        "endpoint": "http://localhost:8080/api/v1"
      },
      "discovery": {
        "method": "broadcast",
        "interval": "30s"
      }
    },
    "uadp": {
      "enabled": true,
      "discovery": "automatic",
      "context_sharing": "enabled",
      "capability_advertising": "enabled"
    }
  },
  
  "deployment": {
    "strategy": "rolling_update",
    "replicas": 2,
    "resources": {
      "requests": {
        "memory": "256Mi",
        "cpu": "100m"
      },
      "limits": {
        "memory": "512Mi",
        "cpu": "250m"
      }
    },
    "health_checks": {
      "liveness": "/health",
      "readiness": "/ready",
      "startup": "/startup"
    },
    "scaling": {
      "min_replicas": 1,
      "max_replicas": 5,
      "target_cpu": 70,
      "target_memory": 80
    }
  },
  
  "compliance": {
    "frameworks": ["iso-42001", "nist-ai-rmf", "eu-ai-act"],
    "certification_level": "gold",
    "audit_trail": true,
    "data_governance": true,
    "privacy": {
      "data_minimization": true,
      "consent_management": true,
      "anonymization": true,
      "right_to_deletion": true
    }
  }
}
