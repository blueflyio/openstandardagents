{
  "permissions": {
    "allow": [
      "Bash(npm run *)",
      "Bash(npm test *)",
      "Bash(npx tsx *)",
      "Bash(node *)",
      "Bash(git status)",
      "Bash(git diff *)",
      "Bash(git log *)",
      "Read(spec/**)",
      "Read(examples/**)",
      "Read(src/**)",
      "Read(tests/**)"
    ],
    "deny": [
      "Bash(rm -rf *)",
      "Bash(git push --force *)",
      "Bash(git commit * --no-verify *)"
    ]
  },
  "env": {
    "OSSA_VERSION": "0.2.8",
    "NODE_ENV": "development"
  },
  "context": {
    "include": [
      "spec/v0.2.8/ossa-0.2.8.schema.json",
      "package.json"
    ]
  }
}
