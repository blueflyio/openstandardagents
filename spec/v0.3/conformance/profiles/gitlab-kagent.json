{
  "$schema": "https://openstandardagents.org/schemas/conformance-profile.json",
  "id": "gitlab-kagent",
  "name": "GitLab Kagent",
  "extends": "enterprise",
  "version": "0.3.5",
  "description": "GitLab Kagent extension compliance",
  "required": {
    "features": [
      "metadata.name",
      "metadata.version",
      "spec.identity.id",
      "spec.role",
      "spec.completion",
      "spec.checkpointing",
      "spec.monitoring",
      "spec.security",
      "spec.extensions.kagent",
      "spec.extensions.kagent.kubernetes.namespace",
      "spec.extensions.kagent.kubernetes.resourceLimits"
    ],
    "weight": 80
  },
  "optional": {
    "features": [
      "spec.extensions.moe",
      "spec.extensions.feedback_loops",
      "spec.extensions.bat"
    ],
    "weight": 20
  },
  "scoring": {
    "pass_threshold": 90,
    "warn_threshold": 75
  }
}
