{
  "files.watcherExclude": {
    "**/node_modules/**": true,
    "**/.git/objects/**": true,
    "**/vendor/**": true,
    "**/.next/**": true,
    "**/dist/**": true,
    "**/build/**": true,
    "**/.turbo/**": true,
    "**/.cache/**": true,
    "**/coverage/**": true
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/vendor": true,
    "**/.next": true,
    "**/dist": true,
    "**/build": true,
    "**/.turbo": true,
    "**/coverage": true
  },
  "typescript.tsserver.maxTsServerMemory": 2048,
  "typescript.disableAutomaticTypeAcquisition": true,
  "git.autoRepositoryDetection": "subFolders",
  "git.repositoryScanMaxDepth": 1,
  "editor.quickSuggestions": true,
  "editor.formatOnSave": true,
  "cursor.chat.maxContextTokens": 8000,
  "yaml.completion": true,
  "yaml.validate": true,
  "yaml.hover": true,
  "yaml.format.enable": true,
  "yaml.format.singleQuote": false,
  "yaml.format.bracketSpacing": true,
  "yaml.format.printWidth": 100,
  "yaml.format.proseWrap": "preserve",
  "yaml.format.trailingComma": true,
  "yaml.keyOrdering": false,
  "yaml.disableAdditionalProperties": false,
  "yaml.disableDefaultProperties": false,
  "yaml.schemaStore.enable": true,
  "yaml.schemaStore.url": "https://www.schemastore.org/api/json/catalog.json",
  "yaml.schemas": {
    "spec/v0.3.0/ossa-0.3.0.schema.json": [
      "**/*.ossa.yaml",
      "**/*.ossa.yml",
      "**/agent.yml",
      "**/agent.yaml",
      "**/manifest.ossa.yaml",
      "**/manifest.ossa.yml"
    ],
    "https://json.schemastore.org/gitlab-ci.json": [
      ".gitlab-ci.yml",
      "**/.gitlab-ci.yml"
    ],
    "https://json.schemastore.org/docker-compose.json": [
      "**/docker-compose.yml",
      "**/docker-compose.yaml"
    ],
    "https://json.schemastore.org/openapi-3.1.json": [
      "**/*.openapi.yaml",
      "**/*.openapi.yml",
      "openapi/**/*.yaml",
      "openapi/**/*.yml"
    ]
  },
  "yaml.customTags": [],
  "yaml.style.flowMapping": "forbid",
  "yaml.style.flowSequence": "forbid",
  "yaml.suggest.parentSkeletonSelectedFirst": false,
  "yaml.maxItemsComputed": 5000,
  "yaml.trace.server": "off",
  "yaml.extension.recommendations": "true",
  "redhat.telemetry.enabled": null
}
