{
  "name": "@ossa/sdk",
  "version": "0.3.3",
  "description": "TypeScript SDK for Open Standard for Scalable AI Agents (OSSA)",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./validator": {
      "types": "./dist/validator.d.ts",
      "import": "./dist/validator.js"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "import": "./dist/client.js"
    }
  },
  "bin": {
    "ossa": "./bin/ossa.js"
  },
  "files": [
    "dist/",
    "bin/",
    "schema/"
  ],
  "scripts": {
    "build": "tsc",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "ossa",
    "ai-agents",
    "llm",
    "openapi",
    "validation",
    "multi-agent"
  ],
  "author": "OSSA Contributors",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/blueflyio/openstandardagents"
  },
  "homepage": "https://openstandardagents.org",
  "dependencies": {
    "ajv": "^8.12.0",
    "ajv-formats": "^3.0.1",
    "yaml": "^2.3.0",
    "commander": "^11.1.0",
    "chalk": "^5.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
