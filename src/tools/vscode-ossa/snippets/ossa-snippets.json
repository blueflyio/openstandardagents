{
  "OSSA Minimal Agent": {
    "prefix": "ossa-agent",
    "body": [
      "apiVersion: ossa/v0.3.3",
      "kind: Agent",
      "",
      "metadata:",
      "  name: ${1:my-agent}",
      "  version: ${2:1.0.0}",
      "  description: |",
      "    ${3:Agent description}",
      "",
      "spec:",
      "  role: |",
      "    ${4:You are a helpful AI assistant.}",
      "  ",
      "  llm:",
      "    provider: ${5|anthropic,openai,google,azure,groq,ollama|}",
      "    model: ${6:claude-sonnet-4-20250514}",
      "    temperature: ${7:0.7}",
      "$0"
    ],
    "description": "Create a minimal OSSA Agent manifest"
  },
  "OSSA Agent with Tools": {
    "prefix": "ossa-agent-tools",
    "body": [
      "apiVersion: ossa/v0.3.3",
      "kind: Agent",
      "",
      "metadata:",
      "  name: ${1:my-agent}",
      "  version: ${2:1.0.0}",
      "  description: |",
      "    ${3:Agent description}",
      "",
      "spec:",
      "  role: |",
      "    ${4:You are a helpful AI assistant with access to tools.}",
      "  ",
      "  llm:",
      "    provider: ${5|anthropic,openai,google|}",
      "    model: ${6:claude-sonnet-4-20250514}",
      "    temperature: ${7:0.7}",
      "  ",
      "  tools:",
      "    - type: ${8|mcp,function,http,openapi|}",
      "      name: ${9:tool_name}",
      "      description: |",
      "        ${10:Tool description}",
      "$0"
    ],
    "description": "Create an OSSA Agent with tools"
  },
  "OSSA Agent with Safety": {
    "prefix": "ossa-agent-safety",
    "body": [
      "apiVersion: ossa/v0.3.3",
      "kind: Agent",
      "",
      "metadata:",
      "  name: ${1:my-agent}",
      "  version: ${2:1.0.0}",
      "  description: |",
      "    ${3:Agent description}",
      "",
      "spec:",
      "  role: |",
      "    ${4:You are a helpful AI assistant.}",
      "  ",
      "  llm:",
      "    provider: ${5|anthropic,openai,google|}",
      "    model: ${6:claude-sonnet-4-20250514}",
      "  ",
      "  safety:",
      "    pii:",
      "      detection: ${7|enabled,disabled,redact|}",
      "      policy: ${8|block,warn,log|}",
      "    ",
      "    rateLimit:",
      "      requestsPerMinute: ${9:10}",
      "      tokensPerDay: ${10:100000}",
      "    ",
      "    guardrails:",
      "      - type: ${11|content,output,behavior|}",
      "        description: ${12:Safety rule description}",
      "$0"
    ],
    "description": "Create an OSSA Agent with safety controls"
  },
  "OSSA Task": {
    "prefix": "ossa-task",
    "body": [
      "apiVersion: ossa/v0.3.3",
      "kind: Task",
      "",
      "metadata:",
      "  name: ${1:my-task}",
      "  version: ${2:1.0.0}",
      "  description: |",
      "    ${3:Task description}",
      "",
      "spec:",
      "  steps:",
      "    - name: ${4:step1}",
      "      type: ${5|script,http,function|}",
      "      description: ${6:Step description}",
      "      config:",
      "        ${7:# Step configuration}",
      "$0"
    ],
    "description": "Create an OSSA Task manifest"
  },
  "OSSA Workflow": {
    "prefix": "ossa-workflow",
    "body": [
      "apiVersion: ossa/v0.3.3",
      "kind: Workflow",
      "",
      "metadata:",
      "  name: ${1:my-workflow}",
      "  version: ${2:1.0.0}",
      "  description: |",
      "    ${3:Workflow description}",
      "",
      "spec:",
      "  entrypoint: ${4:main}",
      "  ",
      "  nodes:",
      "    - name: ${4:main}",
      "      type: ${5|agent,task,workflow|}",
      "      ref: ${6:agent-name}",
      "      next: ${7:next-node}",
      "$0"
    ],
    "description": "Create an OSSA Workflow manifest"
  },
  "OSSA MCP Tool": {
    "prefix": "ossa-tool-mcp",
    "body": [
      "- type: mcp",
      "  name: ${1:mcp_server}",
      "  description: |",
      "    ${2:Tool description}",
      "  server:",
      "    command: ${3:npx}",
      "    args:",
      "      - ${4:-y}",
      "      - ${5:@modelcontextprotocol/server-filesystem}",
      "    env:",
      "      ${6:# Environment variables}",
      "$0"
    ],
    "description": "Add MCP tool to OSSA Agent"
  },
  "OSSA HTTP Tool": {
    "prefix": "ossa-tool-http",
    "body": [
      "- type: http",
      "  name: ${1:api_call}",
      "  description: |",
      "    ${2:Tool description}",
      "  endpoint: ${3:https://api.example.com/endpoint}",
      "  method: ${4|GET,POST,PUT,DELETE,PATCH|}",
      "  headers:",
      "    Content-Type: application/json",
      "    ${5:# Additional headers}",
      "  auth:",
      "    type: ${6|bearer,apikey,oauth2,mtls,none|}",
      "$0"
    ],
    "description": "Add HTTP tool to OSSA Agent"
  },
  "OSSA Function Tool": {
    "prefix": "ossa-tool-function",
    "body": [
      "- type: function",
      "  name: ${1:function_name}",
      "  description: |",
      "    ${2:Function description}",
      "  parameters:",
      "    type: object",
      "    properties:",
      "      ${3:param_name}:",
      "        type: ${4|string,number,boolean,object,array|}",
      "        description: ${5:Parameter description}",
      "    required:",
      "      - ${3:param_name}",
      "$0"
    ],
    "description": "Add function tool to OSSA Agent"
  },
  "OSSA Agent Messaging": {
    "prefix": "ossa-messaging",
    "body": [
      "messaging:",
      "  channels:",
      "    subscribe:",
      "      - ${1:channel-name}",
      "    publish:",
      "      - ${2:channel-name}",
      "  ",
      "  handlers:",
      "    - event: ${3:event-type}",
      "      action: ${4:handle_event}",
      "      priority: ${5|high,medium,low|}",
      "$0"
    ],
    "description": "Add messaging configuration to OSSA Agent"
  },
  "OSSA Constraints": {
    "prefix": "ossa-constraints",
    "body": [
      "constraints:",
      "  cost:",
      "    maxTokensPerDay: ${1:100000}",
      "    maxTokensPerRequest: ${2:4000}",
      "    maxCostPerDay: ${3:10.00}",
      "    currency: ${4:USD}",
      "  ",
      "  performance:",
      "    maxLatencySeconds: ${5:5.0}",
      "    maxConcurrentRequests: ${6:10}",
      "    timeoutSeconds: ${7:30}",
      "$0"
    ],
    "description": "Add constraints to OSSA manifest"
  },
  "OSSA Observability": {
    "prefix": "ossa-observability",
    "body": [
      "observability:",
      "  tracing:",
      "    enabled: ${1|true,false|}",
      "    exporter: ${2|otlp,jaeger,zipkin|}",
      "    endpoint: ${3:http://localhost:4318}",
      "  ",
      "  metrics:",
      "    enabled: ${4|true,false|}",
      "    exporter: ${5|prometheus,otlp|}",
      "    endpoint: ${6:http://localhost:9090/metrics}",
      "  ",
      "  logging:",
      "    level: ${7|debug,info,warn,error|}",
      "    format: ${8|json,text|}",
      "$0"
    ],
    "description": "Add observability to OSSA manifest"
  },
  "OSSA Autonomy": {
    "prefix": "ossa-autonomy",
    "body": [
      "autonomy:",
      "  level: ${1|supervised,autonomous,fully_autonomous|}",
      "  approval_required: ${2|true,false|}",
      "  allowed_actions:",
      "    - ${3:action_name}",
      "  max_iterations: ${4:10}",
      "$0"
    ],
    "description": "Add autonomy configuration to OSSA Agent"
  },
  "OSSA Complete Agent": {
    "prefix": "ossa-agent-complete",
    "body": [
      "apiVersion: ossa/v0.3.3",
      "kind: Agent",
      "",
      "metadata:",
      "  name: ${1:my-agent}",
      "  version: ${2:1.0.0}",
      "  description: |",
      "    ${3:Comprehensive agent description}",
      "  labels:",
      "    environment: ${4|development,staging,production|}",
      "    team: ${5:platform}",
      "",
      "spec:",
      "  role: |",
      "    ${6:You are a helpful AI assistant.}",
      "  ",
      "  llm:",
      "    provider: ${7|anthropic,openai,google|}",
      "    model: ${8:claude-sonnet-4-20250514}",
      "    temperature: ${9:0.7}",
      "    maxTokens: ${10:4096}",
      "  ",
      "  tools:",
      "    - type: ${11|mcp,function,http|}",
      "      name: ${12:tool_name}",
      "      description: ${13:Tool description}",
      "  ",
      "  autonomy:",
      "    level: ${14|supervised,autonomous|}",
      "    approval_required: ${15|true,false|}",
      "  ",
      "  safety:",
      "    pii:",
      "      detection: enabled",
      "      policy: block",
      "    rateLimit:",
      "      requestsPerMinute: 10",
      "  ",
      "  constraints:",
      "    cost:",
      "      maxTokensPerDay: 100000",
      "      currency: USD",
      "  ",
      "  observability:",
      "    tracing:",
      "      enabled: true",
      "      exporter: otlp",
      "    logging:",
      "      level: info",
      "      format: json",
      "$0"
    ],
    "description": "Create a complete OSSA Agent with all features"
  }
}
