{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "helm-chart-architect Output Schema",
  "type": "object",
  "properties": {
    "status": {
      "type": "string",
      "enum": [
        "success",
        "error",
        "processing"
      ]
    },
    "result": {
      "type": "object"
    },
    "execution_id": {
      "type": "string"
    },
    "timestamp": {
      "type": "string",
      "format": "date-time"
    }
  },
  "required": [
    "status"
  ]
}