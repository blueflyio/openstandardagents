# OSSA Development Environment Configuration

# Environment
NODE_ENV=development
LOG_LEVEL=debug

# Phoenix Observability (Default in Dev)
ENABLE_PHOENIX_TRACING=true
PHOENIX_URL=http://localhost:6006
OTLP_GRPC_ENDPOINT=localhost:4317
OTLP_HTTP_ENDPOINT=http://localhost:4318/v1/traces

# Prometheus Metrics
PROMETHEUS_URL=http://localhost:9090
PROMETHEUS_PUSHGATEWAY=http://localhost:9091

# Agent Paths
AGENT_BUILDKIT_PATH=/Users/flux423/Sites/LLM/agent_buildkit/.agents
OSSA_AGENTS_PATH=/Users/flux423/Sites/LLM/OSSA/.agents

# API Server
API_HOST=0.0.0.0
API_PORT=3000

# Database (if needed)
DATABASE_URL=postgresql://ossa:ossa@localhost:5432/ossa_dev

# Redis (for caching)
REDIS_URL=redis://localhost:6379/0

# Knowledge Graph
GRAPH_AUTO_REBUILD=true
GRAPH_OUTPUT_DIR=./knowledge-graph
GRAPH_EXPORT_FORMATS=json,graphml,cytoscape

# Development Features
ENABLE_HOT_RELOAD=true
ENABLE_DEBUG_ENDPOINTS=true
ENABLE_SWAGGER_UI=true
SWAGGER_UI_PORT=8082
