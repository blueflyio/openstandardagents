{
  "name": "@bluefly/openstandardagents",
  "version": "0.3.3",
  "exports": {
    "./schema": "./spec/v{{VERSION}}/ossa-{{VERSION}}.schema.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./validation": {
      "types": "./dist/services/validation.service.d.ts",
      "import": "./dist/services/validation.service.js",
      "require": "./dist/services/validation.service.js"
    },
    "./generation": {
      "types": "./dist/services/generation.service.d.ts",
      "import": "./dist/services/generation.service.js",
      "require": "./dist/services/generation.service.js"
    },
    "./migration": {
      "types": "./dist/services/migration.service.d.ts",
      "import": "./dist/services/migration.service.js",
      "require": "./dist/services/migration.service.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/types/index.js",
      "require": "./dist/types/index.js"
    },
    "./openapi-extensions": {
      "types": "./dist/types/openapi-extensions.d.ts",
      "import": "./dist/types/openapi-extensions.js",
      "require": "./dist/types/openapi-extensions.js"
    },
    "./openapi-extensions-schema": "./spec/extensions/openapi/ossa-openapi-extensions.schema.json",
    "./mesh": {
      "types": "./dist/mesh/index.d.ts",
      "import": "./dist/mesh/index.js",
      "require": "./dist/mesh/index.js"
    }
  },
  "description": "OSSA - Open Standard for Software Agents. The OpenAPI for agents. A specification standard (not a framework) that defines contracts/metadata for production agent systems.",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "ossa": "bin/ossa",
    "ossa-version": "bin/ossa-version",
    "ossa-validate-all": "bin/ossa-validate-all"
  },
  "files": [
    "dist/",
    "spec/",
    "bin/",
    "examples/",
    "openapi/",
    "schemas/",
    "templates/",
    "README.md",
    "LICENSE",
    "CHANGELOG.md",
    ".env.local",
    ".devfile.yaml",
    ".wiki-config.json",
    ".gitlab-ci-trigger",
    "junit.xml",
    "llms-ctx-full.txt",
    "llms-ctx.txt"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/blueflyio/openstandardagents/"
  },
  "bugs": {
    "url": "https://github.com/blueflyio/openstandardagents/issues"
  },
  "homepage": "https://openstandardagents.org",
  "scripts": {
    "// ─────────────────────────────────────────────────────────────────────────────": "",
    "// BUILD & DEVELOPMENT": "",
    "prebuild": "npm run docs:process",
    "build": "tsc && npm run build:assets",
    "build:assets": "cp -r spec dist/",
    "build:clean": "npm run clean && npm run build",
    "dev": "tsc --watch",
    "clean": "rm -rf dist coverage .tsbuildinfo",
    "// TESTING": "",
    "test": "jest",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:e2e": "jest tests/e2e",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --coverage --reporters=default --reporters=jest-junit",
    "// CODE QUALITY": "",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src tests --ext .ts",
    "lint:fix": "eslint src tests --ext .ts --fix",
    "format": "prettier --write 'src/**/*.ts' 'tests/**/*.ts'",
    "format:check": "prettier --check 'src/**/*.ts' 'tests/**/*.ts'",
    "quality": "npm run typecheck && npm run lint && npm run format:check",
    "quality:fix": "npm run lint:fix && npm run format",
    "// VALIDATION": "",
    "validate": "npm run validate:schema && npm run validate:examples",
    "validate:schema": "npx ajv-cli compile -s spec/v$(node -p \"require('./.version.json').current\")/ossa-$(node -p \"require('./.version.json').current\").schema.json --strict=false --allow-union-types",
    "validate:manifest": "npx ajv-cli validate -s spec/v$(node -p \"require('./.version.json').current\")/ossa-$(node -p \"require('./.version.json').current\").schema.json --strict=false --allow-union-types -d",
    "validate:examples": "find examples -name '*.ossa.yaml' -o -name '*.ossa.yml' | head -20 | xargs -I {} npm run validate:manifest -- {}",
    "validate:power": "tsx bin/validate-ossa.ts",
    "validate:openapi": "tsx scripts/validate-openapi-extensions.ts",
    "// VERSION MANAGEMENT (via ossa CLI - single source of truth)": "",
    "version:status": "node dist/cli/index.js release version status",
    "version:bump": "node dist/cli/index.js release version bump",
    "version:sync": "node dist/cli/index.js release version sync",
    "version:check": "node dist/cli/index.js release version check",
    "version:examples": "node dist/cli/index.js release version examples",
    "// DOCUMENTATION": "",
    "docs:process": "tsx scripts/process-doc-templates.ts",
    "docs:check": "test -f README.md && test -f CHANGELOG.md",
    "docs:generate": "tsx scripts/generate-api-docs.ts && tsx scripts/generate-cli-docs.ts && tsx scripts/generate-schema-docs.ts && tsx scripts/generate-examples-docs.ts && tsx scripts/generate-agents-catalog.ts && tsx scripts/generate-types-docs.ts && tsx scripts/generate-errors-docs.ts && tsx scripts/generate-config-docs.ts && tsx scripts/lowercase-docs.ts",
    "// CODE GENERATION": "",
    "gen:types": "tsx scripts/gen-types.ts",
    "gen:zod": "tsx scripts/gen-zod.ts",
    "gen:all": "npm run gen:types && npm run gen:zod",
    "// SYNC & COMPLIANCE": "",
    "sync:pr": "tsx src/cli/commands/github-sync.command.ts pr",
    "sync:batch": "tsx src/cli/commands/github-sync.command.ts batch",
    "sync:list": "tsx src/cli/commands/github-sync.command.ts list",
    "compliance:audit": "tsx scripts/compliance-audit.ts",
    "// MAINTENANCE": "",
    "fix:schemas": "node scripts/fix-schema-formats.js",
    "prepare": "husky install || true",
    "// RELEASE & PUBLISH": "",
    "prerelease": "npm run quality && npm run test && npm run validate",
    "prepublishOnly": "npm run build:clean && npm run test && npm run lint",
    "release:check": "npm run prerelease && echo '✅ Ready for release'"
  },
  "keywords": [
    "ossa",
    "ai-agents",
    "openapi",
    "standard",
    "kubernetes",
    "kagent",
    "cli",
    "validation",
    "agent-generation"
  ],
  "author": "OSSA Standards Team",
  "license": "Apache-2.0",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.0",
    "@aws-sdk/client-bedrock-runtime": "^3.955.0",
    "@gitbeaker/node": "^35.8.1",
    "@google/generative-ai": "^0.21.0",
    "@octokit/rest": "^20.0.0",
    "@qdrant/js-client-rest": "^1.9.0",
    "ajv": "^8.12.0",
    "ajv-formats": "^3.0.1",
    "axios": "^1.12.2",
    "chalk": "^5.6.2",
    "commander": "^11.1.0",
    "glob": "^11.0.3",
    "inversify": "^7.10.4",
    "openai": "^6.9.1",
    "readline": "^1.3.0",
    "reflect-metadata": "^0.2.2",
    "semver": "^7.7.3",
    "yaml": "^2.3.0",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@gitbeaker/rest": "^43.8.0",
    "@jest/globals": "^29.7.0",
    "@octokit/rest": "^22.0.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/exec": "^7.1.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/gitlab": "^13.2.9",
    "@types/jest": "^29.5.14",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.19.19",
    "@types/pg": "^8.15.5",
    "@types/redis": "^4.0.10",
    "@types/semver": "^7.7.1",
    "@typescript-eslint/eslint-plugin": "^8.48.0",
    "@typescript-eslint/parser": "^8.48.0",
    "ajv-cli": "^5.0.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "json-schema-to-typescript": "^15.0.4",
    "json-schema-to-zod": "^2.6.1",
    "prettier": "^3.6.2",
    "semantic-release": "^25.0.1",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
