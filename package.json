{
  "name": "@bluefly/openstandardagents",
  "version": "0.3.6",
  "exports": {
    "./schema": "./spec/v{{VERSION}}/ossa-{{VERSION}}.schema.json",
    "./schema/v0.3.3": "./spec/v{{VERSION}}/ossa-{{VERSION}}.schema.json",
    "./schema/v0.3.4": "./spec/v{{VERSION}}/ossa-{{VERSION}}.schema.json",
    "./schema/v0.3.5": "./spec/v{{VERSION}}/ossa-{{VERSION}}.schema.json",
    "./ag2/swarm-topology": "./spec/v0.3.4/extensions/ag2/swarm-topology.schema.json",
    "./ag2/group-chat": "./spec/v0.3.4/extensions/ag2/group-chat.schema.json",
    "./ag2/nested-chat": "./spec/v0.3.4/extensions/ag2/nested-chat.schema.json",
    "./hitl/intervention-points": "./spec/v0.3.4/protocols/hitl/intervention-points.schema.json",
    "./hitl/human-input-modes": "./spec/v0.3.4/protocols/hitl/human-input-modes.schema.json",
    "./hitl/approval-workflows": "./spec/v0.3.4/protocols/hitl/approval-workflows.schema.json",
    "./a2a/service-discovery": "./spec/v0.3.4/protocols/a2a/service-discovery.schema.json",
    "./a2a/handoff-protocol": "./spec/v0.3.4/protocols/a2a/handoff-protocol.schema.json",
    "./a2a/capability-advertisement": "./spec/v0.3.4/protocols/a2a/capability-advertisement.schema.json",
    "./state/context-serialization": "./spec/v0.3.4/runtime/state/context-serialization.schema.json",
    "./state/memory-portability": "./spec/v0.3.4/runtime/state/memory-portability.schema.json",
    "./state/teachability": "./spec/v0.3.4/runtime/state/teachability.schema.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./validation": {
      "types": "./dist/services/validation.service.d.ts",
      "import": "./dist/services/validation.service.js",
      "require": "./dist/services/validation.service.js"
    },
    "./generation": {
      "types": "./dist/services/generation.service.d.ts",
      "import": "./dist/services/generation.service.js",
      "require": "./dist/services/generation.service.js"
    },
    "./migration": {
      "types": "./dist/services/migration.service.d.ts",
      "import": "./dist/services/migration.service.js",
      "require": "./dist/services/migration.service.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/types/index.js",
      "require": "./dist/types/index.js"
    },
    "./openapi-extensions": {
      "types": "./dist/types/openapi-extensions.d.ts",
      "import": "./dist/types/openapi-extensions.js",
      "require": "./dist/types/openapi-extensions.js"
    },
    "./openapi-extensions-schema": "./spec/extensions/openapi/ossa-openapi-extensions.schema.json",
    "./mesh": {
      "types": "./dist/mesh/index.d.ts",
      "import": "./dist/mesh/index.js",
      "require": "./dist/mesh/index.js"
    },
    "./audit": {
      "types": "./dist/services/audit.d.ts",
      "import": "./dist/services/audit.js",
      "require": "./dist/services/audit.js"
    }
  },
  "description": "OSSA - Open Standard for Software Agents. The OpenAPI for agents. A specification standard (not a framework) that defines contracts/metadata for production agent systems.",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "ossa": "bin/ossa",
    "ossa-version": "bin/ossa-version",
    "ossa-validate-all": "bin/ossa-validate-all",
    "ossa-dev": "src/dev-cli/src/index.ts"
  },
  "files": [
    "dist/",
    "spec/",
    "bin/",
    "examples/",
    "openapi/",
    "schemas/",
    "templates/",
    "README.md",
    "LICENSE",
    "CHANGELOG.md",
    ".version.json",
    ".env.local",
    ".devfile.yaml",
    ".wiki-config.json",
    ".gitlab-ci-trigger",
    "junit.xml",
    "llms-ctx-full.txt",
    "llms-ctx.txt"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/blueflyio/ossa/openstandardagents.git"
  },
  "bugs": {
    "url": "https://github.com/blueflyio/openstandardagents/issues"
  },
  "homepage": "https://openstandardagents.org",
  "scripts": {
    "// ─────────────────────────────────────────────────────────────────────────────": "",
    "// BUILD & DEVELOPMENT": "",
    "prebuild": "npm run docs:process",
    "build": "tsc && tsx src/tools/fix-esm-imports.ts && npm run build:assets",
    "build:assets": "cp -r spec dist/",
    "build:clean": "npm run clean && npm run build",
    "dev": "tsc --watch",
    "clean": "rm -rf dist coverage .tsbuildinfo",
    "// TESTING": "",
    "test": "jest",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:e2e": "jest tests/e2e --passWithNoTests",
    "test:smoke": "jest tests/e2e --testPathPattern=smoke.spec.ts --runInBand",
    "test:smoke:cli": "jest tests/e2e/cli.smoke.spec.ts",
    "test:smoke:npm-pack": "jest tests/e2e/npm-pack.smoke.spec.ts",
    "test:smoke:schema": "jest tests/e2e/schema-coverage.smoke.spec.ts",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --coverage --reporters=default --reporters=jest-junit",
    "ci:validate": "npm run build:clean && npm run quality && npm run test:unit && npm run test:smoke && npm run validate:all",
    "ci:validate:fast": "npm run typecheck && npm run lint && npm run test:unit && npm run validate:schema",
    "test:current": "jest tests/unit/v$(node -p \"require('./.version.json').current\") tests/integration/v$(node -p \"require('./.version.json').current\")",
    "// CODE QUALITY": "",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src tests --ext .ts",
    "lint:fix": "eslint src tests --ext .ts --fix",
    "format": "prettier --write 'src/**/*.ts' 'tests/**/*.ts'",
    "format:check": "prettier --check 'src/**/*.ts' 'tests/**/*.ts'",
    "quality": "npm run typecheck && npm run lint && npm run format:check",
    "quality:fix": "npm run lint:fix && npm run format",
    "// VALIDATION": "",
    "validate": "npm run validate:schema && npm run validate:examples && npm run validate:changelog",
    "validate:all": "npm run validate && npm run validate:sdks",
    "validate:changelog": "npx keep-a-changelog validate CHANGELOG.md --url https://github.com/blueflyio/openstandardagents",
    "validate:sdks": "npm run validate:sdk:typescript && npm run validate:sdk:python",
    "validate:sdk:typescript": "cd src/sdks/typescript && npm install && npm run build && npm run typecheck",
    "validate:sdk:python": "cd src/sdks/python && python -m pip install --quiet -e . && python -m pytest tests/ -v || true",
    "validate:schema": "npx ajv-cli compile -s $(node -p \"require('./.version.json').spec_path\")/$(node -p \"require('./.version.json').schema_file\") -r $(node -p \"require('./.version.json').spec_path\")/extensions/kagent/kagent.schema.json -r $(node -p \"require('./.version.json').spec_path\")/extensions/mcp/mcp.schema.json -r $(node -p \"require('./.version.json').spec_path\")/extensions/a2a/a2a.schema.json -r $(node -p \"require('./.version.json').spec_path\")/extensions/langchain/langchain.schema.json -r $(node -p \"require('./.version.json').spec_path\")/extensions/langgraph/langgraph.schema.json -r $(node -p \"require('./.version.json').spec_path\")/extensions/ag2/ag2.schema.json -r $(node -p \"require('./.version.json').spec_path\")/extensions/crewai/crewai.schema.json --strict=false --allow-union-types -c ajv-formats",
    "validate:manifest": "npx ajv-cli validate -s $(node -p \"require('./.version.json').spec_path\")/$(node -p \"require('./.version.json').schema_file\") --strict=false --allow-union-types -c ajv-formats -d",
    "validate:examples": "find examples -name '*.ossa.yaml' -o -name '*.ossa.yml' | head -20 | xargs -I {} npm run validate:manifest -- {}",
    "validate:power": "tsx src/tools/validation/validate-ossa.ts",
    "validate:openapi": "tsx src/tools/validation/validate-openapi-extensions.ts",
    "validate:current": "tsx src/tools/validation/validate-v$(node -p \"require('./.version.json').current\")-features.ts",
    "validate:current:examples": "find spec/v$(node -p \"require('./.version.json').current\")/examples -name '*.ossa.yaml' | xargs -I {} tsx src/tools/validation/validate-v$(node -p \"require('./.version.json').current\")-features.ts {}",
    "// VERSION MANAGEMENT (via ossa-dev CLI - single source of truth)": "",
    "version:release": "tsx src/dev-cli/src/index.ts version release",
    "version:audit": "tsx src/dev-cli/src/index.ts version audit",
    "version:validate": "tsx src/dev-cli/src/index.ts version validate",
    "version:sync": "tsx src/dev-cli/src/index.ts version sync",
    "version:status": "node dist/cli/index.js release version status",
    "version:check": "node dist/cli/index.js release version check",
    "version:examples": "node dist/cli/index.js release version examples",
    "// INFRASTRUCTURE (TODO: Move to buildkit)": "",
    "env:list": "tsx src/dev-cli/src/index.ts env-cleanup --list",
    "env:cleanup": "tsx src/dev-cli/src/index.ts env-cleanup --stop-stuck",
    "// DOCUMENTATION": "",
    "docs:process": "tsx src/tools/docs/process-doc-templates.ts",
    "docs:check": "test -f README.md && test -f CHANGELOG.md",
    "docs:generate": "tsx src/tools/docs/generate-api-docs.ts && tsx src/tools/docs/generate-cli-docs.ts && tsx src/tools/docs/generate-schema-docs.ts && tsx src/tools/docs/generate-examples-docs.ts && tsx src/tools/generators/generate-agents-catalog.ts && tsx src/tools/docs/generate-types-docs.ts && tsx src/tools/docs/generate-errors-docs.ts && tsx src/tools/docs/generate-config-docs.ts && tsx src/tools/docs/lowercase-docs.ts",
    "docs:current": "tsx src/tools/docs/generate-v$(node -p \"require('./.version.json').current\")-docs.ts",
    "// CODE GENERATION": "",
    "gen:types": "tsx src/tools/generators/gen-types.ts",
    "gen:zod": "tsx src/tools/generators/gen-zod.ts",
    "gen:all": "npm run gen:types && npm run gen:zod",
    "gen:current:schema": "tsx src/tools/generators/merge-v$(node -p \"require('./.version.json').current\")-schema.ts",
    "// SYNC & COMPLIANCE": "",
    "sync:pr": "tsx src/cli/commands/github-sync.command.ts pr",
    "sync:batch": "tsx src/cli/commands/github-sync.command.ts batch",
    "sync:list": "tsx src/cli/commands/github-sync.command.ts list",
    "migrate:batch": "node dist/cli/index.js migrate-batch",
    "compliance:audit": "tsx src/tools/compliance-audit.ts",
    "verify:npm-auth": "tsx src/tools/verify-npm-auth.ts",
    "audit:mrs": "tsx src/tools/audit-mrs.ts",
    "// MAINTENANCE": "",
    "fix:schemas": "node src/tools/generators/fix-schema-formats.js",
    "hooks:install": "lefthook install",
    "hooks:uninstall": "lefthook uninstall",
    "prepare": "lefthook install || true",
    "postinstall": "node bin/postinstall || true",
    "// RELEASE & PUBLISH": "",
    "prerelease": "npm run quality && npm run test && npm run validate",
    "prepublishOnly": "npm run build:clean && npm run test && npm run lint",
    "release:check": "npm run prerelease && echo '✅ Ready for release'",
    "release:verify": "tsx src/dev-cli/src/index.ts release verify",
    "release:prep": "tsx src/dev-cli/src/index.ts release prep v$(node -p \"require('./.version.json').latest_stable\")"
  },
  "keywords": [
    "ossa",
    "ai-agents",
    "openapi",
    "standard",
    "kubernetes",
    "kagent",
    "cli",
    "validation",
    "agent-generation"
  ],
  "author": "OSSA Standards Team",
  "license": "Apache-2.0",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.0",
    "@aws-sdk/client-bedrock-runtime": "^3.955.0",
    "@gitbeaker/node": "^35.8.1",
    "@google/generative-ai": "^0.21.0",
    "@octokit/rest": "^20.0.0",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/exporter-jaeger": "^1.28.0",
    "@opentelemetry/exporter-metrics-otlp-http": "^0.54.0",
    "@opentelemetry/exporter-prometheus": "^0.54.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.54.0",
    "@opentelemetry/exporter-zipkin": "^1.28.0",
    "@opentelemetry/resources": "^1.28.0",
    "@opentelemetry/sdk-node": "^0.54.0",
    "@opentelemetry/sdk-trace-base": "^1.28.0",
    "@qdrant/js-client-rest": "^1.9.0",
    "@types/tar": "^6.1.13",
    "ajv": "^8.12.0",
    "axios": "^1.12.2",
    "boxen": "^7.1.1",
    "chalk": "^5.6.2",
    "commander": "^11.1.0",
    "glob": "^11.0.3",
    "inquirer": "^9.2.12",
    "inversify": "^7.10.4",
    "openai": "^6.9.1",
    "ora": "^8.0.1",
    "readline": "^1.3.0",
    "reflect-metadata": "^0.2.2",
    "semver": "^7.7.3",
    "tar": "^7.5.3",
    "yaml": "^2.3.0",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@gitbeaker/rest": "^43.8.0",
    "@jest/globals": "^29.7.0",
    "@octokit/rest": "^22.0.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/exec": "^7.1.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^12.0.0",
    "@semantic-release/gitlab": "^13.2.9",
    "@types/inquirer": "^9.0.9",
    "@types/jest": "^29.5.14",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.19.19",
    "@types/pg": "^8.15.5",
    "@types/redis": "^4.0.10",
    "@types/semver": "^7.7.1",
    "@typescript-eslint/eslint-plugin": "^8.48.0",
    "@typescript-eslint/parser": "^8.48.0",
    "ajv-cli": "^5.0.0",
    "ajv-formats": "^3.0.1",
    "esbuild": "^0.27.2",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "json-schema-to-typescript": "^15.0.4",
    "json-schema-to-zod": "^2.6.1",
    "keep-a-changelog": "^2.8.0",
    "lefthook": "^1.12.3",
    "prettier": "^3.6.2",
    "semantic-release": "^25.0.1",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "workspaces": [
    "packages/*"
  ],
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "volta": {
    "node": "20.11.0",
    "npm": "10.2.4"
  }
}
