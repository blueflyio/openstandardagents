{
  "name": "ossa/symfony-bundle",
  "description": "Symfony Bundle for Open Standard Agents (OSSA) - Integrate AI agents into Symfony applications",
  "type": "symfony-bundle",
  "keywords": [
    "symfony",
    "bundle",
    "ossa",
    "agents",
    "ai",
    "llm",
    "anthropic",
    "openai",
    "autonomous-agents"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "OSSA Core Team",
      "homepage": "https://openstandardagents.org"
    }
  ],
  "homepage": "https://github.com/blueflyio/openstandardagents",
  "require": {
    "php": ">=8.2",
    "symfony/config": "^6.4|^7.0",
    "symfony/console": "^6.4|^7.0",
    "symfony/dependency-injection": "^6.4|^7.0",
    "symfony/event-dispatcher": "^6.4|^7.0",
    "symfony/http-kernel": "^6.4|^7.0",
    "symfony/yaml": "^6.4|^7.0",
    "symfony/http-client": "^6.4|^7.0",
    "symfony/serializer": "^6.4|^7.0",
    "symfony/validator": "^6.4|^7.0",
    "guzzlehttp/guzzle": "^7.8",
    "psr/log": "^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5",
    "symfony/phpunit-bridge": "^6.4|^7.0",
    "symfony/framework-bundle": "^6.4|^7.0",
    "symfony/browser-kit": "^6.4|^7.0",
    "phpstan/phpstan": "^1.10",
    "friendsofphp/php-cs-fixer": "^3.48",
    "symfony/var-dumper": "^6.4|^7.0"
  },
  "autoload": {
    "psr-4": {
      "Ossa\\SymfonyBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ossa\\SymfonyBundle\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit",
    "phpstan": "phpstan analyse src --level=8",
    "cs-check": "php-cs-fixer fix --dry-run --diff",
    "cs-fix": "php-cs-fixer fix"
  },
  "extra": {
    "branch-alias": {
      "dev-main": "0.3.x-dev"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
