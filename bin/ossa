#!/usr/bin/env node

/**
 * OSSA CLI - Open Standards for Scalable Agents v0.1.8
 * API-first GraphQL agent management platform
 */

import { fileURLToPath } from 'url';
import { dirname, join } from 'path';

const __filename = fileURLToPath(import.meta.url);
const __dirname = dirname(__filename);

// Import the main CLI from cli/src
import(join(__dirname, '../cli/src/index.js')).catch(console.error);