{
  "name": "@company/customer-support-bot",
  "version": "1.0.0",
  "description": "Production customer support agent built with OSSA and LangChain",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:integration": "jest --testPathPattern=integration",
    "validate": "ossa validate agent.ossa.yaml",
    "export:langchain": "ossa export agent.ossa.yaml --platform langchain --output exports/",
    "docker:build": "docker build -t customer-support-bot .",
    "docker:run": "docker-compose up -d"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.0",
    "express": "^4.18.2",
    "langchain": "^0.3.0",
    "@langchain/openai": "^0.3.0",
    "@langchain/community": "^0.3.0",
    "openai": "^6.9.1",
    "redis": "^4.6.0",
    "zod": "^4.1.11",
    "prom-client": "^15.1.0",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/sdk-node": "^0.54.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.19.19",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.14"
  },
  "keywords": [
    "ossa",
    "agent",
    "customer-support",
    "langchain",
    "chatbot"
  ],
  "author": "Your Company",
  "license": "Apache-2.0"
}
