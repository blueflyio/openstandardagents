{
  "apiVersion": "ossa/v0.3.3",
  "kind": "Agent",
  "metadata": {
    "name": "workflow-orchestrator",
    "version": "1.0.0",
    "description": "Langflow workflow orchestration agent"
  },
  "spec": {
    "role": "You orchestrate complex workflows by coordinating multiple AI agents and tools.",
    "llm": {
      "provider": "openai",
      "model": "gpt-4",
      "temperature": 0.5
    },
    "tools": []
  },
  "extensions": {
    "langflow": {
      "enabled": true,
      "flow_id": "workflow-orchestrator-v1",
      "flow_name": "Multi-Agent Workflow Orchestrator",
      "endpoint": "https://api.langflow.example.com",
      "components": [
        {
          "id": "llm-1",
          "type": "OpenAI",
          "name": "Primary LLM"
        },
        {
          "id": "agent-1",
          "type": "Agent",
          "name": "Workflow Coordinator"
        }
      ]
    }
  }
}
