{
  "metadata": {
    "name": "agent-name-skill-training-data",
    "version": "1.0.0",
    "description": "Generic training data patterns for cross-platform AI agents",
    "format": "JSONL-compatible training examples",
    "schema_version": "openapi-ai-agents/v0.1.1",
    "created": "2025-01-26",
    "framework_compatibility": [
      "langchain",
      "crewai",
      "autogen",
      "openai",
      "anthropic",
      "google",
      "huggingface"
    ]
  },
  
  "training_patterns": {
    "task_execution": [
      {
        "pattern": "analyze_request",
        "description": "Standard request analysis workflow",
        "example": {
          "input": {
            "task": "analyze",
            "parameters": {
              "data": "input data to analyze",
              "analysis_type": "comprehensive",
              "output_format": "structured"
            }
          },
          "process": [
            "1. Validate input parameters",
            "2. Determine analysis approach",
            "3. Execute analysis with appropriate tools",
            "4. Format results according to requirements",
            "5. Provide confidence metrics and metadata"
          ],
          "output": {
            "result": "structured analysis results",
            "metadata": {
              "confidence": 0.95,
              "processing_time": "150ms",
              "tokens_used": 245,
              "compliance_status": "validated"
            },
            "status": "success"
          }
        }
      },
      
      {
        "pattern": "generate_content",
        "description": "Content generation with quality control",
        "example": {
          "input": {
            "task": "generate",
            "parameters": {
              "content_type": "documentation",
              "requirements": "comprehensive technical documentation",
              "style": "professional",
              "length": "detailed"
            }
          },
          "process": [
            "1. Parse content requirements",
            "2. Research relevant information",
            "3. Structure content outline",
            "4. Generate detailed content",
            "5. Apply quality checks and compliance validation"
          ],
          "output": {
            "result": "generated content meeting all requirements",
            "metadata": {
              "word_count": 1500,
              "readability_score": 8.5,
              "compliance_check": "passed",
              "framework_compatibility": "verified"
            },
            "status": "success"
          }
        }
      }
    ],
    
    "error_handling": [
      {
        "pattern": "graceful_degradation",
        "description": "Handle errors while maintaining functionality",
        "scenarios": [
          {
            "error_type": "invalid_input",
            "response": "Validate input and provide clear error message with suggestions",
            "fallback": "Request valid input format with examples"
          },
          {
            "error_type": "resource_unavailable", 
            "response": "Attempt alternative approaches or cached results",
            "fallback": "Notify user of limitation and suggest retry timing"
          },
          {
            "error_type": "compliance_violation",
            "response": "Immediately halt processing and report violation",
            "fallback": "Provide guidance on achieving compliance"
          }
        ]
      }
    ],
    
    "framework_integration": [
      {
        "framework": "langchain",
        "patterns": [
          {
            "name": "tool_integration",
            "description": "Standard LangChain tool wrapper pattern",
            "implementation": {
              "tool_name": "agent_name_skill_tool",
              "description": "Brief tool description for LangChain",
              "input_schema": "JSON schema defining expected inputs",
              "output_format": "Structured response format",
              "async_support": true,
              "streaming": false
            }
          }
        ]
      },
      
      {
        "framework": "crewai",
        "patterns": [
          {
            "name": "agent_role",
            "description": "CrewAI role-based agent definition",
            "implementation": {
              "role": "Specialist in [Your Domain]",
              "goal": "Execute tasks with expertise and precision",
              "backstory": "Comprehensive agent background and capabilities",
              "tools": ["rest_api", "data_processor", "validator"],
              "delegation": true,
              "verbose": true
            }
          }
        ]
      },
      
      {
        "framework": "autogen",
        "patterns": [
          {
            "name": "conversation_agent",
            "description": "AutoGen conversation participant",
            "implementation": {
              "name": "YourDomainSpecialist",
              "system_message": "Detailed system message defining capabilities and behavior",
              "human_input_mode": "NEVER",
              "max_consecutive_auto_reply": 10,
              "code_execution_config": {"use_docker": true}
            }
          }
        ]
      }
    ],
    
    "performance_optimization": [
      {
        "pattern": "token_optimization",
        "techniques": [
          {
            "name": "semantic_compression",
            "description": "Compress similar content while preserving meaning",
            "target_reduction": "35-45%",
            "method": "Content deduplication and semantic chunking"
          },
          {
            "name": "response_caching",
            "description": "Cache frequently requested operations",
            "cache_strategy": "LRU with semantic similarity",
            "ttl": "1h",
            "hit_ratio_target": "85%"
          }
        ]
      }
    ],
    
    "compliance_patterns": [
      {
        "framework": "iso_42001",
        "patterns": [
          {
            "requirement": "ai_system_documentation",
            "implementation": "Comprehensive agent documentation with capabilities matrix",
            "validation": "Automated compliance checking against ISO 42001 controls"
          }
        ]
      },
      
      {
        "framework": "nist_ai_rmf",
        "patterns": [
          {
            "function": "govern",
            "implementation": "AI governance policies and oversight mechanisms",
            "validation": "Regular governance review and policy compliance checks"
          }
        ]
      },
      
      {
        "framework": "eu_ai_act", 
        "patterns": [
          {
            "requirement": "transparency",
            "implementation": "Clear documentation of AI system capabilities and limitations",
            "validation": "User-facing transparency notices and capability disclosure"
          }
        ]
      }
    ]
  },
  
  "response_templates": {
    "success_response": {
      "result": "{{processed_result}}",
      "metadata": {
        "processing_time": "{{duration_ms}}ms",
        "confidence": "{{confidence_score}}",
        "tokens_used": "{{token_count}}",
        "compliance_status": "validated",
        "framework_compatibility": "verified"
      },
      "status": "success"
    },
    
    "error_response": {
      "error": {
        "code": "{{error_code}}",
        "message": "{{error_message}}",
        "details": "{{error_details}}",
        "suggestions": ["{{suggestion_1}}", "{{suggestion_2}}"]
      },
      "metadata": {
        "request_id": "{{request_id}}",
        "timestamp": "{{iso_timestamp}}",
        "retry_after": "{{retry_seconds}}s"
      },
      "status": "error"
    },
    
    "partial_response": {
      "result": "{{partial_result}}",
      "progress": {
        "completed": "{{percentage}}%",
        "current_step": "{{current_operation}}",
        "total_steps": "{{total_operations}}",
        "estimated_completion": "{{eta_seconds}}s"
      },
      "status": "partial"
    }
  },
  
  "validation_rules": {
    "input_validation": [
      {
        "rule": "required_parameters_present",
        "check": "Verify all required parameters are provided",
        "error_message": "Missing required parameter: {{parameter_name}}"
      },
      {
        "rule": "parameter_type_validation",
        "check": "Validate parameter types match expected schema",
        "error_message": "Invalid type for {{parameter_name}}: expected {{expected_type}}, got {{actual_type}}"
      }
    ],
    
    "output_validation": [
      {
        "rule": "result_completeness",
        "check": "Ensure all required output fields are present",
        "error_message": "Incomplete result: missing {{missing_fields}}"
      },
      {
        "rule": "compliance_check",
        "check": "Validate output meets compliance requirements",
        "error_message": "Output fails compliance check: {{compliance_issue}}"
      }
    ]
  },
  
  "usage_examples": [
    {
      "name": "basic_analysis",
      "description": "Simple analysis task with standard parameters",
      "request": {
        "task": "analyze",
        "parameters": {
          "input": "sample data for analysis",
          "depth": "standard",
          "format": "json"
        }
      },
      "expected_response": {
        "result": "analysis results in requested format",
        "metadata": {
          "confidence": 0.92,
          "processing_time": "125ms"
        },
        "status": "success"
      }
    },
    
    {
      "name": "complex_generation",
      "description": "Complex content generation with specific requirements",
      "request": {
        "task": "generate",
        "parameters": {
          "type": "technical_documentation",
          "requirements": {
            "sections": ["overview", "implementation", "examples"],
            "style": "comprehensive",
            "audience": "developers"
          }
        }
      },
      "expected_response": {
        "result": "comprehensive technical documentation",
        "metadata": {
          "sections_generated": 3,
          "word_count": 2500,
          "compliance_validated": true
        },
        "status": "success"
      }
    }
  ]
}