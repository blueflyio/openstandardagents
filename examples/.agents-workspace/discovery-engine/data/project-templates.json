    "by_maturity": {
      "production": {
        "indicators": [
          "ci_cd_configured",
          "docker_configured",
          "monitoring_setup",
          "multiple_environments"
        ],
        "thresholds": {
          "commits_per_month": 10,
          "contributors": 2,
          "test_coverage": 70
        }
      },
      "development": {
        "indicators": [
          "active_development",
          "recent_commits",
          "basic_testing"
        ],
        "thresholds": {
          "commits_per_week": 2,
          "last_commit_days": 7
        }
      },
      "experimental": {
        "indicators": [
          "prototype_code",
          "limited_documentation",
          "single_contributor"
        ],
        "thresholds": {
          "total_commits": 20,
          "contributors": 1
        }
      }
    }
