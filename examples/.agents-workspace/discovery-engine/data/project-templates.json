  "migration_strategies": {
    "simple_projects": {
      "criteria": {
        "file_count": "< 100",
        "complexity_score": "< 0.3",
        "dependencies": "< 20"
      },
      "strategy": "automated_migration",
      "template": "./templates/simple-migration.yml",
      "estimated_time": "30 minutes"
    },
    
    "medium_projects": {
      "criteria": {
        "file_count": "100-1000",
        "complexity_score": "0.3-0.7",
        "dependencies": "20-100"
      },
      "strategy": "guided_migration",
      "template": "./templates/guided-migration.yml",
      "estimated_time": "2-4 hours"
    },
    
    "complex_projects": {
      "criteria": {
        "file_count": "> 1000",
        "complexity_score": "> 0.7",
        "dependencies": "> 100"
      },
      "strategy": "phased_migration",
      "template": "./templates/phased-migration.yml",
      "estimated_time": "1-2 weeks"
    }
  }
