{
  "metadata": {
    "name": "workspace-discovery-project-templates",
    "version": "1.0.0",
    "description": "Project detection and categorization templates for workspace discovery",
    "created": "2025-01-26",
    "framework_compatibility": [
      "nodejs",
      "python",
      "java", 
      "dotnet",
      "go",
      "rust",
      "docker",
      "kubernetes"
    ]
  },
  
  "project_detection_templates": {
    "nodejs": {
      "primary_indicators": ["package.json"],
      "secondary_indicators": ["yarn.lock", "pnpm-lock.yaml", "node_modules/", ".nvmrc"],
      "framework_detection": {
        "express": {
          "patterns": ["express", "@express", "app.listen"],
          "confidence": 0.9
        },
        "react": {
          "patterns": ["react", "react-dom", "jsx", "tsx"],
          "confidence": 0.95
        },
        "vue": {
          "patterns": ["vue", "@vue", "vue.config.js"],
          "confidence": 0.9
        },
        "angular": {
          "patterns": ["@angular", "angular.json", "ng serve"],
          "confidence": 0.95
        },
        "next": {
          "patterns": ["next", "next.config.js", "pages/", "_app.js"],
          "confidence": 0.9
        },
        "nuxt": {
          "patterns": ["nuxt", "nuxt.config.js", "layouts/", "pages/"],
          "confidence": 0.9
        }
      },
      "migration_template": {
        "estimated_effort": "2-4 hours",
        "complexity": "low",
        "agent_suggestions": [
          "api-validator",
          "performance-monitor", 
          "security-scanner",
          "documentation-generator"
        ]
      }
    },
    
